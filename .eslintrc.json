{
    "extends": ["next/core-web-vitals", "prettier"],
    "plugins": ["prettier"],
    "rules": {
      "prettier/prettier": ["error"],
      "react-hooks/rules-of-hooks": "error",
      "react-hooks/exhaustive-deps": "warn",
      "no-unused-vars": "warn",
      "no-console": "warn",
      "eqeqeq": ["error", "always"],
      "semi": ["error", "always"],
      "quotes": ["error", "double"],
      "jsx-quotes": ["error", "prefer-double"],
      "indent": ["error", 2],
      "object-curly-spacing": ["error", "always"]
    }
  }
  